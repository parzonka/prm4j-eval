#!/usr/bin/env bash

echo "pure dacapo"
./dacapo "avrora --iterations 10 --no-validation"
./dacapo "h2 --iterations 10 --no-validation"
./dacapo "eclipse --iterations 10 --no-validation"

echo "dacapo with load-time weaving"
./prm4j UnsafeIterator "avrora --iterations 10 --no-validation"
./prm4j UnsafeIterator "h2 --iterations 10 --no-validation"
./prm4j UnsafeIterator "eclipse --iterations 10 --no-validation"
./javamop UnsafeIterator "avrora --iterations 10 --no-validation"
./javamop UnsafeIterator "h2 --iterations 10 --no-validation"
./javamop UnsafeIterator "eclipse --iterations 10 --no-validation"
./prm4j UnsafeMapIterator "avrora --iterations 10 --no-validation"
./prm4j UnsafeMapIterator "h2 --iterations 10 --no-validation"
./prm4j UnsafeMapIterator "eclipse --iterations 10 --no-validation"
./javamop UnsafeMapIterator "avrora --iterations 10 --no-validation"
./javamop UnsafeMapIterator "h2 --iterations 10 --no-validation"
./javamop UnsafeMapIterator "eclipse --iterations 10 --no-validation"

